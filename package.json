{
  "name": "base-jaxx",
  "version": "0.3.0",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "openapi": "node static/openapi.js > static/openapi.json",
    "pm2": "node static/pm2.js > static/pm2.json",
    "fixtures": "node static/fixtures.js > static/fixtures.json",
    "pretest": "npm run pm2 && npm run openapi && npm run fixtures && eslint .",
    "test": "tap -bCRlist --no-coverage test",
    "coverage": "npm run pretest && tap test"
  },
  "dependencies": {
    "fastify": "^2.3.0",
    "got": "^9.6.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "fastify-oas": "^2.1.3",
    "shipit-cli": "^4.2.0",
    "shipit-deploy": "^4.1.4",
    "tap": "^13.1.9"
  },
  "config": {
    "prefix": "base",
    "port": 8100
  }
}
