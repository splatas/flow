{
  "name": "base-jaxx",
  "x-prefix": "base",
  "x-port": 8100,
  "version": "0.3.0",
  "main": "server.js",
  "x-git-version": "{{X-GIT-VERSION}}",
  "scripts": {
    "start": "node server",
    "openapi": "node stat/openapi.js > stat/openapi.json",
    "pm2": "node stat/pm2.js > stat/pm2.json",
    "fixtures": "node stat/fixtures.js > stat/fixtures.json",
    "pretest": "npm run pm2 && npm run openapi && npm run fixtures && eslint .",
    "test": "tap -bCRlist test",
    "coverage": "npm run pretest && tap --cov test"
  },
  "dependencies": {
    "fastify": "^2.1.0",
    "got": "^9.6.0",
    "jsonwebtoken": "^8.5.0"
  },
  "devDependencies": {
    "fastify-oas": "^2.0.0-rc.4",
    "shipit-cli": "^4.1.2",
    "shipit-deploy": "^4.1.3",
    "tap": "^12.5.3"
  }
}
