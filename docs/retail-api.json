                  "required": ["code", "exp"]
                },
                "examples": {
                  "New Code": {
                    "value": {
                      "code": "1A2B8",
                      "exp": 1620668011
                    }
                  }
                }
              }
            }
          }
        },
        "operationId": "get-retail-v1-code",
        "description": "Permite generar un codigo de activaci贸n a para un deco retail.",
        "parameters": [
          {
            "schema": {
              "type": "string",
              "example": "141346D1E516"
            },
            "in": "query",
            "name": "macAddress",
            "required": true,
            "description": "Mac del dispositivo"
          }
        ]
      }
    },
    "/retail/v1/validate-code": {
      "post": {
        "tags": [],
        "summary": "Validate Code #FBE-98",
        "operationId": "post-retail-v1-validate-code",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "valid": {
                      "example": true,
                      "type": "boolean"
                    }
                  }
                },
                "examples": {
                  "Validate code": {
                    "value": {
                      "valid": true
                    }
                  },
                  "Invalid Code": {
                    "value": {
                      "valid": false
                    }
                  }
                }
              }
            }
          }
        },
        "description": "Permite validar un codigo generado para un deco retail, asocia datos del usuario con la mac address del deco.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "code": {
                    "type": "string",
                    "example": "1A2B8"
                  },
                  "name": {
                    "type": "string",
                    "example": "125478954"
                  },
                  "avatarId": {
                    "type": "integer"
                  }
                },
                "required": ["code", "name", "avatarId"]
              }
            }
          },
          "description": ""
        },
        "parameters": [
          {
            "schema": {
              "type": "string",
              "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzdWIiOiIxMjM0NTY3ODkwIiwibmFtZSI6IkpvaG4gRG9lIiwiaWF0IjoxNTE2MjM5MDIyfQ.SflKxwRJSMeKKF2QT4fwpMeJf36POk6yJV_adQssw5c"
            },
            "in": "header",
            "name": "Autorization",
            "description": "Bearer Token JWT",
            "required": true
          }
        ]
      }
    },
    "/retail/v1/code/state": {
      "get": {
        "summary": "Valida codigo alfa numerico activo #FBE-99",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "boolean",
                      "description": "Indica si el deco fue activado a traves del codigo alfa numerico o no"
                    },
                    "customerId": {
                      "type": "string",
                      "description": "customer Id del usuario de flow"
                    }
                  },
                  "required": ["status"]
                },
                "examples": {
                  "Deco activated": {
                    "value": {
                      "status": true,
                      "customerId": "123243234"
                    }
                  },
                  "Deco unactivated": {
                    "value": {
                      "status": false
                    }
                  }
                }
              }
            }
          }
        },
        "operationId": "get-retail-v1-code-state",
        "description": "Permite al deco saber si alguna PC activ贸 al codigo alfa numerico.",
        "parameters": [
          {
            "schema": {
              "type": "string",
              "example": "141346D1E516"
