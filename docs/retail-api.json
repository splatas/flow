  "paths": {
    "/retail/v1/deco-type": {
      "get": {
        "summary": "Deco Type #FBE-73",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string"
                    }
                  }
                },
                "examples": {
                  "deco no retail": {
                    "value": {
                      "status": "NO_RETAIL"
                    }
                  },
                  "deco retail activo": {
                    "value": {
                      "status": "ACTIVE"
                    }
                  },
                  "deco retail inactivo": {
                    "value": {
                      "status": "INACTIVE"
                    }
                  }
                }
              }
            }
          }
        },
        "operationId": "get-retail-v1-verify",
        "description": "Permite identificar y el deco es de tipo comodato o caja retail aportando si se encuentra activo o inactivo.",
        "parameters": [
          {
            "schema": {
              "type": "string",
              "example": "14:13:46:D1:E5:16"
            },
            "in": "query",
            "name": "macAddress",
            "required": true
          }
        ]
      },
      "parameters": []
    },
    "/retail/v1/code": {
      "get": {
        "summary": "Generate Code of activation #FBE-95",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "string"
                    },
                    "exp": {
                      "type": "integer",
                      "description": "Tiempo de expiraci贸n en tiempo unix",
                      "example": 1620668011
