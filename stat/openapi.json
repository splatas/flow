  "paths": {
    "/base/v1/ping": {
      "get": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "description": "pong",
                  "type": "string"
                }
              }
            },
            "description": "pong"
          }
        },
        "summary": "Ping",
        "description": "punga",
        "tags": [
          "hello"
        ]
      }
    },
    "/base/v1/now": {
      "get": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "description": "unix timestamp + milli",
                  "type": "integer"
                }
              }
            },
            "description": "unix timestamp + milli"
          }
        },
        "summary": "Now",
        "description": "whats tha time?!",
        "tags": [
          "hello"
        ]
      }
    },
    "/base/v1/totalMB": {
      "get": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "description": "memory consumptions & totals",
                  "type": "object",
                  "properties": {
                    "total_heap_size": {
                      "type": "number"
                    },
                    "total_heap_size_executable": {
                      "type": "number"
                    },
                    "total_physical_size": {
                      "type": "number"
                    },
                    "total_available_size": {
                      "type": "number"
                    },
                    "used_heap_size": {
                      "type": "number"
                    },
                    "heap_size_limit": {
                      "type": "number"
                    },
                    "malloced_memory": {
                      "type": "number"
                    },
                    "peak_malloced_memory": {
                      "type": "number"
                    },
                    "does_zap_garbage": {
                      "type": "boolean"
                    }
                  }
                }
              }
            },
            "description": "memory consumptions & totals"
          }
        },
        "summary": "Memory usage",
        "description": "megabytes & ram",
        "tags": [
          "hello"
        ]
      }
    },
    "/base/v1/revision": {
      "get": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "description": "Git revision",
                  "type": "string"
                }
              }
            },
            "description": "Git revision"
          }
        },
        "summary": "Revision",
        "description": "Git revision",
        "tags": [
          "hello"
        ]
      }
    },
    "/base/v1/private": {
      "get": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "description": "Git revision",
                  "type": "string"
                }
              }
            },
            "description": "Git revision"
          }
        },
        "summary": "Revision",
        "description": "Git revision",
        "tags": [
          "hello"
        ]
      }
    }
